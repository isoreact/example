{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "11"
        },
        "exclude": ["@babel/plugin-transform-regenerator"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "fileName": false
      }
    ]
  ]
}
